<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>
<c74object as_bpatcher="1" name="wa.launchpad.device">
    <digest>Launchpad Mini device</digest>
    <description>needs to pe present in order for a <o>wa.lauchnpad.button</o> to work</description>
    <metadatalist>
        <metadata name="author">Ralf Waspe</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
    </metadatalist>
    <parser inlet_count="1"/>
    <misc name="Outputs"/>
    <objarglist/>
    <attributelist/>
    <methodlist>
        <method name="off" standard="0">
            <arglist>
                <arg name="" optional="0" type="symbol"/>
            </arglist>
            <digest>turn launchpad off</digest>
            <description>turn launchpad off</description>
        </method>
        <method name="on" standard="0">
            <arglist>
                <arg name="" optional="0" type="symbol"/>
            </arglist>
            <digest>turn laucnpad on</digest>
        </method>
        <method name="printColors" standard="0">
            <arglist>
                <arg name="" optional="0" type="symbol"/>
            </arglist>
            <digest>print available color symbols</digest>
            <description>print available color symbols (deprecated)</description>
        </method>
        <method name="reset" standard="0">
            <arglist>
                <arg name="" optional="0" type="symbol"/>
            </arglist>
            <digest>reset the launchpad</digest>
            <description>reset the launchpad</description>
        </method>
    </methodlist>
    <seealsolist>
        <seealso name="wa.launchpad.button"/>
        <seealso name="wa.launchpad.column"/>
    </seealsolist>
</c74object>
