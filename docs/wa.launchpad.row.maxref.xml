<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>
<c74object name="wa.launchpad.row" patcher_type="standard">
    <digest>a row of a lauchnpad</digest>
    <description>used 9 value as a slider, ranging from -4 to +4</description>
    <metadatalist>
        <metadata name="author">Ralf Waspe</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
    </metadatalist>
    <parser inlet_count="0"/>
    <misc name="Outputs">
        <entry id="0" name="value">
            <digest/>
            <description>the current value (on change)</description>
        </entry>
    </misc>
    <objarglist>
        <objarg name="1" optional="1" type="symbol">
            <digest>the row number (1)</digest>
        </objarg>
    </objarglist>
    <attributelist>
        <attribute get="1" name="color" set="1" size="1" type="symbol">
            <digest>the color of the row</digest>
            <description>the color of the row</description>
        </attribute>
        <attribute get="1" name="downColor" set="1" size="1" type="symbol">
            <digest>color of active value</digest>
            <description>color of active value</description>
        </attribute>
    </attributelist>
    <methodlist>
        <method name="bang" standard="1">
            <digest/>
            <description>update colors</description>
        </method>
        <method name="color" standard="0">
            <arglist>
                <arg name="" optional="0" type="symbol"/>
            </arglist>
            <digest>the color of the row</digest>
            <description>the color of the row</description>
        </method>
        <method name="downColor" standard="0">
            <arglist>
                <arg name="" optional="0" type="symbol"/>
            </arglist>
            <digest>color of active value</digest>
            <description>color of active value</description>
        </method>
    </methodlist>
    <seealsolist/>
</c74object>
