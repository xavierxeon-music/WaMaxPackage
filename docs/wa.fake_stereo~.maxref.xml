<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>
<c74object name="wa.fake_stereo~" patcher_type="0">
    <digest>Haas Effect</digest>
    <description>create fake stereo</description>
    <metadatalist>
        <metadata name="author">Ralf Waspe</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
    </metadatalist>
    <parser inlet_count="1"/>
    <misc name="Outputs">
        <entry id="0" name="signalLeft">
            <digest/>
            <description>left output</description>
        </entry>
        <entry id="1" name="signalRight">
            <digest/>
            <description>right output</description>
        </entry>
    </misc>
    <objarglist/>
    <attributelist>
        <attribute get="1" name="move" set="1" size="1" type="float">
            <digest>stereo movement</digest>
            <description>stereo movement (0-1)</description>
        </attribute>
        <attribute get="1" name="width" set="1" size="1" type="float">
            <digest>stereo width</digest>
            <description>stereo width (0-1)</description>
        </attribute>
    </attributelist>
    <methodlist>
        <method name="list" standard="1">
            <digest/>
            <description>width and move</description>
        </method>
        <method name="signal" standard="1">
            <digest/>
        </method>
    </methodlist>
    <seealsolist/>
</c74object>
