<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>
<c74object as_bpatcher="1" name="wa.nosferatu.stages">
    <digest>create stages</digest>
    <description>creates stages to be used by other nosferatu blocks</description>
    <metadatalist>
        <metadata name="author">Ralf Waspe</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
    </metadatalist>
    <parser inlet_count="1"/>
    <misc name="Outputs">
        <entry id="0" name="nosferatu_bus">
            <digest/>
            <description>the message bus</description>
        </entry>
    </misc>
    <objarglist/>
    <attributelist>
        <attribute get="1" name="count" set="1" size="1" type="symbol">
            <digest>stage count</digest>
            <description>stage count</description>
        </attribute>
    </attributelist>
    <methodlist>
        <method name="bang" standard="1">
            <digest/>
            <description>reset to first statge</description>
        </method>
    </methodlist>
    <seealsolist>
        <seealso name="wa.nosferatu.gates"/>
        <seealso name="wa.nosferatu.maybe"/>
        <seealso name="wa.nosferatu.pitch"/>
    </seealsolist>
</c74object>
