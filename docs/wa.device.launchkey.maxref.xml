<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>
<c74object as_bpatcher="1" name="wa.device.launchkey">
    <digest>launchkey device</digest>
    <description>A novation Launchkey 61 mk3  keyboard<br/><br/>In a locked patch move the mouse to see element names.<br/><br/>Single mouse click freezes element name display<br/>Double mouse click create new <o>wa.launchkey,element</o></description>
    <metadatalist>
        <metadata name="author">Ralf Waspe</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
    </metadatalist>
    <parser inlet_count="1"/>
    <misc name="Outputs">
        <entry id="0" name="input">
            <digest/>
            <description>button pressed, or element used</description>
        </entry>
    </misc>
    <objarglist/>
    <attributelist/>
    <methodlist>
        <method name="bang" standard="1">
            <digest/>
            <description>reset the device</description>
        </method>
        <method name="color" standard="0">
            <arglist>
                <arg name="" optional="1" type="symbol"/>
            </arglist>
            <digest>set color</digest>
            <description>manually set a pad color</description>
        </method>
    </methodlist>
    <seealsolist/>
</c74object>
