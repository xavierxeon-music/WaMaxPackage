<c74object as_bpatcher="0" name="wa.setup.transport_vcv">
    <digest>transport from vcv rack</digest>
    <description>controll the max transport from vcv rack</description>
    <metadatalist>
        <metadata name="author">Ralf Waspe</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
    </metadatalist>
    <parser inlet_count="3"/>
    <misc name="Outputs">
        <entry name="intervall" id="0">
            <digest/>
            <description>the intervall between ticks</description>
        </entry>
    </misc>
    <objarglist/>
    <attributelist/>
    <methodlist>
        <method standard="1" name="signal">
            <digest/>
            <description>on left input: the 16th note tick<br/>on middle input: reset the transport<br/>on right input: start / stop the transport</description>
        </method>
    </methodlist>
    <seealsolist/>
</c74object>
