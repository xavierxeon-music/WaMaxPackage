<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>
<c74object name="wa.setup.audio_device">
    <digest>Activate the audio device</digest>
    <metadatalist>
        <metadata name="author">Ralf Waspe</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
    </metadatalist>
    <parser inlet_count="2"/>
    <misc name="Outputs"/>
    <objarglist>
        <objarg type="symbol" optional="1" name="devioce">
            <digest>the name of the device</digest>
            <description>the name of the device as given in the system settings.<br/><br/>default is "BaclHole 64ch"<br/></description>
        </objarg>
    </objarglist>
    <attributelist>
        <attribute type="int" set="0" name="auto_start" get="0" size="1">
            <digest>automatically start the device</digest>
            <description>automatically start the device<br/><br/>default is true</description>
        </attribute>
        <attribute type="int" set="0" name="input_enabled" get="0" size="1">
            <digest>enable the input</digest>
            <description>default is true</description>
        </attribute>
    </attributelist>
    <methodlist>
        <method standard="1" name="bang">
            <arglist>
                <arg type="bang" optional="0" name=""/>
            </arglist>
            <digest/>
            <description>In left input: enable device manually.<br/><br/>In right input: choose the device as set in the system settings. <b>MAC OS ONLY</b><br/></description>
        </method>
    </methodlist>
    <seealsolist/>
</c74object>
