<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>
<c74object name="wa.grid.pot" patcher_type="0">
    <digest>grid element</digest>
    <description>a knob on a grid</description>
    <metadatalist>
        <metadata name="author">Ralf Waspe</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
    </metadatalist>
    <parser inlet_count="0"/>
    <misc name="Outputs">
        <entry id="1" name="value">
            <digest/>
            <description>the value of the element (on change)</description>
        </entry>
        <entry id="2" name="rgb">
            <digest/>
            <description>rgb value of current color as float</description>
        </entry>
    </misc>
    <objarglist>
        <objarg name="id" optional="1" type="int">
            <digest>the grid id  (11)</digest>
            <description>the grid ID of the element<br/><br/>11 12 13 14<br/>21 22 23 24<br/>31 32 33 34<br/>41 42 43 44<br/><br/></description>
        </objarg>
    </objarglist>
    <attributelist>
        <attribute get="1" name="color" set="1" size="1" type="symbol">
            <digest>led color</digest>
            <description>the color of the LED</description>
        </attribute>
        <attribute get="1" name="name" set="1" size="1" type="symbol">
            <digest>name</digest>
            <description>some name to show in UI<br/></description>
        </attribute>
    </attributelist>
    <methodlist>
        <method name="color" standard="0">
            <arglist>
                <arg name="" optional="0" type="symbol"/>
            </arglist>
            <digest>led color</digest>
            <description>the color of the LED</description>
        </method>
        <method name="name" standard="0">
            <arglist>
                <arg name="" optional="0" type="symbol"/>
            </arglist>
            <digest>name</digest>
            <description>some name to show in UI<br/></description>
        </method>
    </methodlist>
    <seealsolist/>
</c74object>
