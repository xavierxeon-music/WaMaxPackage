<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>
<c74object name="wa.grid.pot" patcher_type="standard">
    <digest>grid element</digest>
    <description>a knob on a grid controller</description>
    <metadatalist>
        <metadata name="author">Ralf Waspe</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
    </metadatalist>
    <misc name="Outputs">
        <entry name="float">
            <digest/>
            <description>(right) the current normalized value</description>
        </entry>
        <entry name="list">
            <digest/>
            <description>(left) the color as float values</description>
        </entry>
    </misc>
    <objarglist>
        <objarg name="id" optional="0" type="int">
            <digest>the grid id  (11)</digest>
            <description>the grid ID of the element<br/><br/>11 12 13 14<br/>21 22 23 24<br/>31 32 33 34<br/>41 42 43 44<br/></description>
        </objarg>
    </objarglist>
    <attributelist>
        <attribute name="color" type="symbol">
            <digest>led color</digest>
            <description>the color of the LED</description>
        </attribute>
        <attribute name="name" type="symbol">
            <digest>name</digest>
            <description>the name of the element</description>
        </attribute>
    </attributelist>
    <methodlist>
        <method name="color" standard="0">
            <arglist>
                <arg name="color" optional="0" type="symbol"/>
            </arglist>
            <digest>led color</digest>
            <description>the color of the LED</description>
        </method>
        <method name="name" standard="0">
            <arglist>
                <arg name="name" optional="0" type="symbol"/>
            </arglist>
            <digest>name</digest>
            <description>the name of the element</description>
        </method>
    </methodlist>
    <seealsolist/>
</c74object>
