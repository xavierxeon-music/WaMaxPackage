<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>
<c74object as_bpatcher="0" name="wa.launchkey.element">
    <digest>launchkey element</digest>
    <description>User interface element of a launchkey device <o>wa.device.launchkey</o></description>
    <metadatalist>
        <metadata name="author">Ralf Waspe</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
    </metadatalist>
    <parser inlet_count="1"/>
    <misc name="Outputs">
        <entry id="1" name="value">
            <digest/>
            <description>the button value<br/></description>
        </entry>
        <entry id="2" name="pressed">
            <digest/>
            <description>bang if  button is pressed.</description>
        </entry>
        <entry id="3" name="released">
            <digest/>
            <description>bang if button is released.</description>
        </entry>
    </misc>
    <objarglist>
        <objarg name="1" optional="1" type="symbol">
            <digest>element name</digest>
            <description>the name of the user interfae element.<br/><br/>see <o>wa.device.launchkey</o></description>
        </objarg>
    </objarglist>
    <attributelist>
        <attribute get="1" name="color" set="1" size="1" type="symbol">
            <digest>button color </digest>
            <description>the color of an unpressed button (default is white)</description>
        </attribute>
        <attribute get="1" name="downColor" set="1" size="1" type="symbol">
            <digest>button color </digest>
            <description>the color of an unpressed button (default is white)</description>
        </attribute>
        <attribute get="1" name="toggle" set="1" size="1" type="symbol">
            <digest>toggle</digest>
            <description>make button aa toggle button, has no effect on pots or faders!</description>
        </attribute>
    </attributelist>
    <methodlist>
        <method name="color" standard="0">
            <arglist>
                <arg name="" optional="0" type="symbol"/>
            </arglist>
            <digest>button color </digest>
            <description>the color of an unpressed button (default is white)</description>
        </method>
        <method name="downColor" standard="0">
            <arglist>
                <arg name="" optional="0" type="symbol"/>
            </arglist>
            <digest>button color </digest>
            <description>the color of an unpressed button (default is white)</description>
        </method>
        <method name="setElement" standard="0">
            <arglist>
                <arg name="" optional="0" type="symbol"/>
            </arglist>
            <digest> element name</digest>
            <description>change the element name</description>
        </method>
        <method name="toggle" standard="0">
            <arglist>
                <arg name="" optional="0" type="symbol"/>
            </arglist>
            <digest>toggle</digest>
            <description>make button aa toggle button, has no effect on pots or faders!</description>
        </method>
    </methodlist>
    <seealsolist/>
</c74object>
