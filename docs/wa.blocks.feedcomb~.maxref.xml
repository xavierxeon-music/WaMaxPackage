<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>
<c74object name="wa.blocks.feedcomb~">
    <digest>a simple feedcomb filter</digest>
    <description>a simple feedcomb filter</description>
    <metadatalist>
        <metadata name="author">Ralf Waspe</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
    </metadatalist>
    <parser inlet_count="4"/>
    <misc name="Outputs">
        <entry name="audio" id="1">
            <digest/>
            <description>signal output</description>
        </entry>
    </misc>
    <objarglist>
        <objarg name="1" optional="1" type="float">
            <digest>delay</digest>
            <description>the delay time in milliseconds<br/></description>
        </objarg>
        <objarg name="2" optional="1" type="float">
            <digest>feedback</digest>
            <description>the amount of feedback [0, 1]</description>
        </objarg>
    </objarglist>
    <attributelist>
        <attribute size="1" name="cutoff" type="symbol" get="1" set="1">
            <digest>cutoff freqeuncy</digest>
            <description>the cutoff frequency of the lowpass filter for the feedback line</description>
        </attribute>
    </attributelist>
    <methodlist/>
    <seealsolist/>
</c74object>
