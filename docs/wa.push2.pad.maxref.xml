        <attribute get="1" set="1" name="toggle" type="symbol" size="1">
            <digest/>
        </attribute>
    </attributelist>
    <methodlist>
        <method standard="0" name="color">
            <arglist>
                <arg name="" optional="0" type="symbol"/>
            </arglist>
            <digest/>
        </method>
        <method standard="0" name="downColor">
            <arglist>
                <arg name="" optional="0" type="symbol"/>
            </arglist>
            <digest/>
        </method>
        <method standard="0" name="forceActive">
            <arglist>
                <arg name="" optional="0" type="symbol"/>
            </arglist>
            <digest/>
        </method>
        <method standard="0" name="setPad">
            <arglist>
                <arg name="" optional="0" type="symbol"/>
            </arglist>
            <digest/>
        </method>
        <method standard="0" name="toggle">
            <arglist>
                <arg name="" optional="0" type="symbol"/>
            </arglist>
            <digest/>
        </method>
    </methodlist>
    <seealsolist/>
</c74object>
