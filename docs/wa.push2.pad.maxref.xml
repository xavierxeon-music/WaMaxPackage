<c74object as_bpatcher="0" name="wa.push2.pad">
    <digest>a pad on a push device</digest>
    <description>a pad on a push device</description>
    <metadatalist>
        <metadata name="author">Ralf Waspe</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
    </metadatalist>
    <parser inlet_count="1"/>
    <misc name="Outputs">
        <entry name="active" id="1">
            <digest/>
        </entry>
        <entry name="pressed" id="2">
            <digest/>
        </entry>
        <entry name="released" id="3">
            <digest/>
        </entry>
    </misc>
    <objarglist>
        <objarg name="name" optional="1" type="symbol">
            <digest>the name of the pad</digest>
            <description>the name of the pad. see <o>wa.push.device~</o> for details</description>
        </objarg>
    </objarglist>
    <attributelist>
        <attribute get="1" set="1" name="color" type="symbol" size="1">
