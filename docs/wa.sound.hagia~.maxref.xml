<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>
<c74object name="wa.sound.hagia~" patcher_type="standard">
    <digest>sofia clone</digest>
    <description>a clone og the choas devices hagia</description>
    <metadatalist>
        <metadata name="author">Ralf Waspe</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
        <metadata name="tag">audio</metadata>
    </metadatalist>
    <parser inlet_count="1"/>
    <misc name="Outputs">
        <entry id="0" name="main">
            <digest/>
        </entry>
        <entry id="1" name="rippleB">
            <digest/>
        </entry>
        <entry id="2" name="rippleA">
            <digest/>
        </entry>
    </misc>
    <objarglist/>
    <attributelist>
