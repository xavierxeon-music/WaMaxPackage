        </method>
        <method name="signal" standard="1">
            <digest/>
            <description>pitch</description>
        </method>
        <method name="in1" standard="0">
            <digest>message input</digest>
            <description>message input</description>
        </method>
        <method name="lenA" standard="0">
            <arglist>
                <arg name="" optional="0" type="symbol"/>
            </arglist>
            <digest/>
        </method>
        <method name="lenB" standard="0">
            <arglist>
                <arg name="" optional="0" type="symbol"/>
            </arglist>
            <digest/>
        </method>
        <method name="message" standard="0">
            <digest>message input</digest>
            <description>message input</description>
        </method>
        <method name="ratioA" standard="0">
            <arglist>
                <arg name="" optional="0" type="symbol"/>
            </arglist>
            <digest/>
        </method>
        <method name="ratioB" standard="0">
            <arglist>
                <arg name="" optional="0" type="symbol"/>
            </arglist>
            <digest/>
        </method>
    </methodlist>
    <seealsolist/>
