<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>
<c74object as_bpatcher="0" name="wa.graphics.text">
    <digest>draw text</digest>
    <description>draw text  on a <o>wa.graphics.target</o></description>
    <metadatalist>
        <metadata name="author">Ralf Waspe</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
    </metadatalist>
    <parser inlet_count="1"/>
    <misc name="Outputs"/>
    <objarglist>
        <objarg name="x" optional="1" type="int">
            <digest>the x position of the text</digest>
            <description>the x position of the text</description>
        </objarg>
        <objarg name="y" optional="1" type="int">
            <digest>the y position of the text</digest>
            <description>the y position of the text</description>
        </objarg>
        <objarg name="fontSize" optional="1" type="int">
            <digest>the font size</digest>
            <description>the font size</description>
        </objarg>
        <objarg name="text" optional="1" type="symbol">
            <digest>the text to be drawn</digest>
            <description>the text to be drawn</description>
        </objarg>
    </objarglist>
    <attributelist>
        <attribute get="1" name="color" set="1" size="1" type="symbol">
            <digest>color (ffffff)</digest>
            <description>rgb hex color of the box, default is  ffffff</description>
        </attribute>
        <attribute get="1" name="deviceName" set="1" size="1" type="symbol">
            <digest>target device name (push2)</digest>
            <description>must be same devcieName as the <o>wa.graphics.target</o></description>
        </attribute>
    </attributelist>
    <methodlist>
        <method name="color" standard="0">
            <arglist>
                <arg name="" optional="0" type="symbol"/>
            </arglist>
            <digest>color</digest>
            <description>rgb hex color of the box, default is  ffffff</description>
        </method>
        <method name="fontSize" standard="0">
            <arglist>
                <arg name="" optional="0" type="symbol"/>
            </arglist>
            <digest>the font size</digest>
            <description>the font size</description>
        </method>
        <method name="text" standard="0">
            <arglist>
                <arg name="" optional="0" type="symbol"/>
            </arglist>
            <digest>the text to be drawn</digest>
            <description>the text to be drawn</description>
        </method>
        <method name="x" standard="0">
            <arglist>
                <arg name="" optional="0" type="symbol"/>
            </arglist>
            <digest>the x position of the text</digest>
            <description>the x position of the text</description>
        </method>
        <method name="y" standard="0">
            <arglist>
                <arg name="" optional="0" type="symbol"/>
            </arglist>
            <digest>the y position of the text</digest>
            <description>the y position of the text</description>
        </method>
    </methodlist>
    <seealsolist>
        <seealso name="wa.graphics.box"/>
        <seealso name="wa.graphics.circle"/>
        <seealso name="wa.graphics.hex_color"/>
        <seealso name="wa.graphics.line"/>
        <seealso name="wa.graphics.target"/>
    </seealsolist>
</c74object>
