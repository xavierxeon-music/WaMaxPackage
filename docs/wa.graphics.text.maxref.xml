<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>
<c74object as_bpatcher="0" name="wa.graphics.text">
    <digest>draw text</digest>
    <description>draw text  on a <o>wa.graphics.target</o></description>
    <metadatalist>
        <metadata name="author">Ralf Waspe</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
    </metadatalist>
    <parser inlet_count="1"/>
    <misc name="Outputs"/>
    <objarglist>
        <objarg optional="1" name="x" type="int">
            <digest>the x position of the text</digest>
            <description>the x position of the text</description>
        </objarg>
        <objarg optional="1" name="y" type="int">
            <digest>the y position of the text</digest>
            <description>the y position of the text</description>
        </objarg>
        <objarg optional="1" name="fontSize" type="int">
            <digest>the font size</digest>
            <description>the font size</description>
        </objarg>
        <objarg optional="1" name="text" type="symbol">
            <digest>the text to be drawn</digest>
            <description>the text to be drawn</description>
        </objarg>
    </objarglist>
    <attributelist>
        <attribute size="1" get="1" name="color" set="1" type="symbol">
            <digest>color</digest>
            <description>rgb hex color of the box, default is  ffffff</description>
        </attribute>
        <attribute size="1" get="1" name="deviceName" set="1" type="symbol">
            <digest>targer device name</digest>
            <description>must be same devcieName as the <o>wa.graphics.target</o></description>
        </attribute>
    </attributelist>
    <methodlist>
        <method standard="0" name="color">
            <arglist>
                <arg optional="0" name="" type="symbol"/>
            </arglist>
            <digest>color</digest>
            <description>rgb hex color of the box, default is  ffffff</description>
        </method>
        <method standard="0" name="fontSize">
            <arglist>
                <arg optional="0" name="" type="symbol"/>
            </arglist>
            <digest>the font size</digest>
            <description>the font size</description>
        </method>
        <method standard="0" name="text">
            <arglist>
                <arg optional="0" name="" type="symbol"/>
            </arglist>
            <digest>the text to be drawn</digest>
            <description>the text to be drawn</description>
        </method>
        <method standard="0" name="x">
            <arglist>
                <arg optional="0" name="" type="symbol"/>
            </arglist>
            <digest>the x position of the text</digest>
            <description>the x position of the text</description>
        </method>
        <method standard="0" name="y">
            <arglist>
                <arg optional="0" name="" type="symbol"/>
            </arglist>
            <digest>the y position of the text</digest>
            <description>the y position of the text</description>
        </method>
    </methodlist>
    <seealsolist/>
</c74object>
