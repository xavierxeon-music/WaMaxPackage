<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>
<c74object name="wa.debug.window" patcher_type="gui">
    <digest>local print</digest>
    <description>acts like print, but in a local window</description>
    <metadatalist>
        <metadata name="author">Ralf Waspe</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
    </metadatalist>
    <parser inlet_count="1"/>
    <misc name="Outputs"/>
    <objarglist/>
    <attributelist>
        <attribute get="1" name="clearonload" set="1" size="1" type="int">
            <digest>clea on load (1)</digest>
            <description>removes all window content after load</description>
        </attribute>
        <attribute get="1" name="maxlines" set="1" size="1" type="int">
            <digest>max lines (10)</digest>
            <description>max number of lines in the window. when the number is reached the window is cleared</description>
        </attribute>
    </attributelist>
    <methodlist>
        <method name="anything" standard="1">
            <digest/>
            <description>any input</description>
        </method>
    </methodlist>
    <seealsolist/>
</c74object>
