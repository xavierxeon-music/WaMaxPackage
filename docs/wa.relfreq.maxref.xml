<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>
<c74object name="wa.relfreq" patcher_type="standard">
    <digest>relative frequency</digest>
    <description>calculate the relative frequency</description>
    <metadatalist>
        <metadata name="author">Ralf Waspe</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
    </metadatalist>
    <parser inlet_count="1"/>
    <misc name="Outputs">
        <entry id="0" name="relfreq">
            <digest/>
            <description>the relative frequency</description>
        </entry>
    </misc>
    <objarglist/>
    <attributelist>
        <attribute get="1" name="basenote" set="1" size="1" type="symbol">
            <digest>basenote (60)</digest>
            <description>use the frequency of this midi note as the base, i.e the relative output is 1.</description>
        </attribute>
    </attributelist>
    <methodlist>
        <method name="float" standard="1">
            <digest/>
            <description>the frequency to measure</description>
        </method>
        <method name="int" standard="1">
            <digest/>
            <description>the frequency to measure</description>
        </method>
        <method name="signal" standard="1">
            <digest/>
            <description>the frequency to measure</description>
        </method>
    </methodlist>
    <seealsolist/>
</c74object>
