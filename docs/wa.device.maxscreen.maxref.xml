<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>
<c74object as_bpatcher="0" name="wa.device.maxscreen">
    <digest>max screen</digest>
    <description>connect to maxscren on ipad</description>
    <metadatalist>
        <metadata name="author">Ralf Waspe</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
    </metadatalist>
    <parser inlet_count="1"/>
    <misc name="Outputs">
        <entry id="1" name="connected">
            <digest/>
            <description>is connected</description>
        </entry>
    </misc>
    <objarglist>
        <objarg name="1" optional="1" type="symbol">
            <digest>host name</digest>
            <description>the name or ip of the device to which to send the input</description>
        </objarg>
    </objarglist>
    <attributelist>
        <attribute get="1" name="fps" set="1" size="1" type="int">
            <digest>frames per second (30)</digest>
            <description>the send dintervall</description>
        </attribute>
    </attributelist>
    <methodlist>
        <method name="done" standard="0">
            <arglist>
                <arg name="" optional="0" type="matrix"/>
            </arglist>
            <digest>matrix</digest>
            <description>the matrix to send</description>
        </method>
        <method name="fps" standard="0">
            <arglist>
                <arg name="" optional="1" type="int"/>
            </arglist>
            <digest>frames per second (30)</digest>
            <description>the send dintervall</description>
        </method>
    </methodlist>
    <seealsolist/>
</c74object>
