<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>
<c74object name="wa.setup.transport_manual" as_bpatcher="0">
    <digest>start the transport</digest>
    <description>start the transport</description>
    <metadatalist>
        <metadata name="author">Ralf Waspe</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
    </metadatalist>
    <parser inlet_count="2"/>
    <misc name="Outputs"/>
    <objarglist>
        <objarg name="tempo" optional="1" type="int">
            <digest>tempo</digest>
            <description>set the tempo (as beats per minute)</description>
        </objarg>
    </objarglist>
    <attributelist/>
    <methodlist>
        <method name="int" standard="1">
            <digest/>
            <description>in left inlet: start / stop the transport</description>
        </method>
        <method name="bang" standard="1">
            <digest/>
            <description>in right inlet: reset the transport</description>
        </method>
    </methodlist>
    <seealsolist/>
</c74object>
