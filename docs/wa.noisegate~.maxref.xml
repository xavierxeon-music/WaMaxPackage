<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>
<c74object name="wa.noisegate~" patcher_type="standard">
    <digest>noise gate</digest>
    <description>cutoff signal below a given amplitude</description>
    <metadatalist>
        <metadata name="author">Ralf Waspe</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
    </metadatalist>
    <parser inlet_count="1"/>
    <misc name="Outputs">
        <entry id="0" name="signal">
            <digest/>
            <description>signal</description>
        </entry>
    </misc>
    <objarglist/>
    <attributelist>
        <attribute get="1" name="interval" set="1" size="1" type="float">
            <digest>lookahead (10)</digest>
            <description>ramp intervall</description>
        </attribute>
        <attribute get="1" name="threshold" set="1" size="1" type="float">
            <digest>threshold (0.001)</digest>
            <description>cuto off singal below this amplitude</description>
        </attribute>
    </attributelist>
    <methodlist>
        <method name="float" standard="1">
            <digest/>
            <description>threshold</description>
        </method>
        <method name="signal" standard="1">
            <digest/>
            <description>input signal</description>
        </method>
    </methodlist>
    <seealsolist/>
</c74object>
