<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>
<c74object as_bpatcher="0" name="wa.device.kraken">
    <digest>soundkraken</digest>
    <description>REceiver for a soundkraken instance running on an iOS device via network</description>
    <metadatalist>
        <metadata name="author">Ralf Waspe</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
    </metadatalist>
    <parser inlet_count="1"/>
    <misc name="Outputs"/>
    <objarglist>
        <objarg name="1" optional="1" type="symbol">
            <digest>device name</digest>
            <description>the name of the network midi device</description>
        </objarg>
    </objarglist>
    <attributelist/>
    <methodlist>
        <method name="bang" standard="1">
            <digest/>
        </method>
    </methodlist>
    <seealsolist/>
</c74object>
