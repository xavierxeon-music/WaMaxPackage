<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>
<c74object as_bpatcher="0" name="wa.graphics.line">
    <digest>draw a line</digest>
    <description>draw a line on a <o>wa.graphics.target</o></description>
    <metadatalist>
        <metadata name="author">Ralf Waspe</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
    </metadatalist>
    <parser inlet_count="1"/>
    <misc name="Outputs"/>
    <objarglist>
        <objarg optional="1" type="int" name="x1">
            <digest>the start x coordinate</digest>
            <description>the start x coordinate</description>
        </objarg>
        <objarg optional="1" type="int" name="y1">
            <digest>the start y coordinate</digest>
            <description>the start y coordinate</description>
        </objarg>
        <objarg optional="1" type="int" name="x2">
            <digest>the end x coordinate</digest>
            <description>the end x coordinate</description>
        </objarg>
        <objarg optional="1" type="int" name="y2">
            <digest>the end y coordinate</digest>
            <description>the end y coordinate</description>
        </objarg>
    </objarglist>
    <attributelist>
        <attribute set="1" get="1" type="symbol" name="color" size="1">
            <digest>color</digest>
            <description>rgb hex color of the box, default is  ffffff</description>
        </attribute>
        <attribute set="1" get="1" type="symbol" name="deviceName" size="1">
            <digest>targer device name</digest>
            <description>must be same devcieName as the <o>wa.graphics.target</o></description>
        </attribute>
    </attributelist>
    <methodlist>
        <method standard="0" name="color">
            <arglist>
                <arg optional="0" type="symbol" name=""/>
            </arglist>
            <digest>color</digest>
            <description>rgb hex color of the box, default is  ffffff</description>
        </method>
        <method standard="0" name="xEnd">
            <arglist>
                <arg optional="0" type="int" name=""/>
            </arglist>
            <digest>the start x coordinate</digest>
            <description>the start x coordinate</description>
        </method>
        <method standard="0" name="xStart">
            <arglist>
                <arg optional="0" type="int" name=""/>
            </arglist>
            <digest>the start y coordinate</digest>
            <description>the start y coordinate</description>
        </method>
        <method standard="0" name="yEnd">
            <arglist>
                <arg optional="0" type="int" name=""/>
            </arglist>
            <digest>the end x coordinate</digest>
            <description>the end x coordinate</description>
        </method>
        <method standard="0" name="yStart">
            <arglist>
                <arg optional="0" type="int" name=""/>
            </arglist>
            <digest>the end y coordinate</digest>
            <description>the end y coordinate</description>
        </method>
    </methodlist>
    <seealsolist/>
</c74object>
