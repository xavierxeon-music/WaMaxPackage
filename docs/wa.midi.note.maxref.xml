<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>
<c74object as_bpatcher="0" name="wa.midi.note">
    <digest>midi note input</digest>
    <description>capture the velocity of a note</description>
    <metadatalist>
        <metadata name="author">Ralf Waspe</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
    </metadatalist>
    <parser inlet_count="1"/>
    <misc name="Outputs">
        <entry id="1" name="value">
            <digest/>
            <description>the velocity of the note</description>
        </entry>
        <entry id="2" name="pressed">
            <digest/>
            <description>bang if  note on</description>
        </entry>
        <entry id="3" name="releades">
            <digest/>
            <description>bang if  note off</description>
        </entry>
    </misc>
    <objarglist>
        <objarg name="1" optional="1" type="symbol">
            <digest>device name</digest>
            <description>device name (default: "Internal LaunchKey")</description>
        </objarg>
    </objarglist>
    <attributelist>
        <attribute get="1" name="value" set="1" size="1" type="int">
            <digest>midi note </digest>
            <description>which midi note to capture</description>
        </attribute>
    </attributelist>
    <methodlist>
        <method name="value" standard="0">
            <arglist>
                <arg name="" optional="0" type="int"/>
            </arglist>
            <digest>midi note </digest>
            <description>which midi note to capture</description>
        </method>
    </methodlist>
    <seealsolist/>
</c74object>
