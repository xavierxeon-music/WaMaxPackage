<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>
<c74object name="wa.graphics.circle" as_bpatcher="0">
    <digest>draw a circle</digest>
    <description>draw a box on a <o>wa.graphics.target</o></description>
    <metadatalist>
        <metadata name="author">Ralf Waspe</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
    </metadatalist>
    <parser inlet_count="1"/>
    <misc name="Outputs"/>
    <objarglist>
        <objarg name="1" type="int" optional="1">
            <digest>x position</digest>
            <description>the x position of the center</description>
        </objarg>
        <objarg name="2" type="symbol" optional="1">
            <digest>y position</digest>
            <description>the y position of the center</description>
        </objarg>
        <objarg name="3" type="int" optional="1">
            <digest>radius</digest>
            <description>the radius of the circle</description>
        </objarg>
    </objarglist>
    <attributelist>
        <attribute name="color" get="1" size="1" type="symbol" set="1">
            <digest>color</digest>
            <description>rgb hex color of the box, default is  ffffff</description>
        </attribute>
        <attribute name="deviceName" get="1" size="1" type="symbol" set="1">
            <digest>targer device name</digest>
            <description>must be same devcieName as the <o>wa.graphics.device</o></description>
        </attribute>
        <attribute name="fill" get="1" size="1" type="int" set="1">
            <digest>fillt the box</digest>
            <description>set to 1 if box is to be filled. default is 1</description>
        </attribute>
    </attributelist>
    <methodlist>
        <method name="color" standard="0">
            <arglist>
                <arg name="" type="symbol" optional="0"/>
            </arglist>
            <digest>color</digest>
            <description>rgb hex color of the box, default is  ffffff</description>
        </method>
        <method name="fill" standard="0">
            <arglist>
                <arg name="" type="int" optional="0"/>
            </arglist>
            <digest>fillt the box</digest>
            <description>set to 1 if box is to be filled. </description>
        </method>
        <method name="radius" standard="0">
            <arglist>
                <arg name="" type="symbol" optional="0"/>
            </arglist>
            <digest>radius</digest>
            <description>set the radius of the circle</description>
        </method>
        <method name="x" standard="0">
            <arglist>
                <arg name="" type="symbol" optional="0"/>
            </arglist>
            <digest>x position</digest>
            <description>set the x position of the center</description>
        </method>
        <method name="y" standard="0">
            <arglist>
                <arg name="" type="symbol" optional="0"/>
            </arglist>
            <digest>y position</digest>
            <description>the y position of the center</description>
        </method>
    </methodlist>
    <seealsolist/>
</c74object>
