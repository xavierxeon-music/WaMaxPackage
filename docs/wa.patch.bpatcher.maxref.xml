<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>
<c74object as_bpatcher="0" name="wa.patch.bpatcher">
    <digest>load as bpatcher</digest>
    <description>when loaded as a bpatcher force presentation mode and resize bpatcher</description>
    <metadatalist>
        <metadata name="author">Ralf Waspe</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
    </metadatalist>
    <parser inlet_count="0"/>
    <misc name="Outputs">
        <entry id="0" name="loaded">
            <digest/>
            <description>send out a bang when the size has been readjusted</description>
        </entry>
    </misc>
    <objarglist/>
    <attributelist>
        <attribute get="1" name="automatic" set="1" size="1" type="symbol">
            <digest>automatic resize</digest>
            <description>automatic resize</description>
        </attribute>
    </attributelist>
    <methodlist/>
    <seealsolist/>
</c74object>
